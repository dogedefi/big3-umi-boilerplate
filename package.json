{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "UMI_ENV=prod umi build",
    "postinstall": "rimraf ./src/.umi && umi generate tmp && rimraf ./.mfsu-*",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "generate": "new-component",
    "analyze": "cross-env UMI_ENV=prod ANALYZE=1 umi build"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@apollo/client": "^3.5.9",
    "abort-controller": "^3.0.0",
    "big3-fast-marquee": "^1.3.3",
    "big3-styled-base": "^1.3.16",
    "big3-web3": "^1.0.6",
    "bignumber.js": "^9.0.1",
    "classnames": "^2.3.1",
    "dayjs": "^1.10.7",
    "ethers": "^5.4.1",
    "graphql": "^16.3.0",
    "lodash.throttle": "^4.1.1",
    "react": "17.x",
    "react-dom": "^17.x",
    "react-helmet-async": "^1.0.9",
    "use-dexie": "^1.2.4",
    "wired-styled-px2vw": "^2.3.2"
  },
  "devDependencies": {
    "@dogedefi/plugin-multichain": "^1.1.2",
    "@dogedefi/zip-webpack-plugin": "1.0.0",
    "@types/lodash.throttle": "^4.1.1",
    "@types/react": "17.x",
    "@types/react-dom": "17.x",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.5",
    "big3-adaptive-media-loader": "^1.0.1",
    "big3-new-component": "^4.0.1",
    "big3-web3-umi-plugin": "^1.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.21",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^2.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.2",
    "umi": "^3.5.5",
    "yorkie": "^2.0.0"
  }
}
